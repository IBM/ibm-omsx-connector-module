<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="Order">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="OrderLine">
          <xs:complexType>
            <xs:attribute name="Action" type="xs:string" use="required" />
            <xs:attribute name="ReservationMandatory" type="xs:string" use="required" />
          </xs:complexType>
        </xs:element>
        <xs:element name="Instructions">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Instruction">
                <xs:complexType>
                  <xs:attribute name="Action" type="xs:string" use="required" />
                  <xs:attribute name="InstructionDetailKey" type="xs:string" use="required" />
                  <xs:attribute name="InstructionText" type="xs:string" use="required" />
                  <xs:attribute name="InstructionType" type="xs:string" use="required" />
                  <xs:attribute name="InstructionURL" type="xs:string" use="required" />
                  <xs:attribute name="SequenceNo" type="xs:string" use="required" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="Action" type="xs:string" use="required" />
      <xs:attribute name="OrderNo" type="xs:unsignedShort" use="required" />
      <xs:attribute name="EnterpriseCode" type="xs:string" use="required" />
      <xs:attribute name="DocumentType" type="xs:unsignedByte" use="required" />
    </xs:complexType>
  </xs:element>
</xs:schema>