        <xs:attribute type="mule:expressionString" use="optional" default="#[payload]" name="targetValue">
          <xs:annotation>
            <xs:documentation>An expression that will be evaluated against the operation's output and the outcome of that expression will be stored in the target variable</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/ibm-omsx" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="GetOrderDetailType" substitutionGroup="mule:abstract-operator" name="get-order-detail"></xs:element>
  <xs:complexType name="GetOrderDetailType">
