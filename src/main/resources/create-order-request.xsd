<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="Order">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="OrderLines">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="OrderLine" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Item">
                      <xs:complexType>
                        <xs:simpleContent>
                          <xs:extension base="xs:string">
                            <xs:attribute type="xs:string" name="UnitOfMeasure" use="optional"/>
                            <xs:attribute type="xs:string" name="ItemDesc" use="optional"/>
                            <xs:attribute type="xs:int" name="ItemID" use="optional"/>
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="LinePriceInfo">
                      <xs:complexType>
                        <xs:simpleContent>
                          <xs:extension base="xs:string">
                            <xs:attribute type="xs:float" name="UnitPrice" use="optional"/>
                            <xs:attribute type="xs:string" name="IsPriceLocked" use="optional"/>
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="PersonInfoShipTo">
                      <xs:complexType>
                        <xs:simpleContent>
                          <xs:extension base="xs:string">
                            <xs:attribute type="xs:string" name="AddressLine1" use="optional"/>
                            <xs:attribute type="xs:string" name="FirstName" use="optional"/>
                            <xs:attribute type="xs:int" name="ZipCode" use="optional"/>
                            <xs:attribute type="xs:string" name="State" use="optional"/>
                            <xs:attribute type="xs:string" name="Country" use="optional"/>
                            <xs:attribute type="xs:string" name="LastName" use="optional"/>
                            <xs:attribute type="xs:string" name="City" use="optional"/>
                            <xs:attribute type="xs:string" name="EMailID" use="optional"/>
                            <xs:attribute type="xs:long" name="MobilePhone" use="optional"/>
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute type="xs:string" name="CarrierServiceCode" use="optional"/>
                  <xs:attribute type="xs:string" name="SCAC" use="optional"/>
                  <xs:attribute type="xs:byte" name="SubLineNo" use="optional"/>
                  <xs:attribute type="xs:string" name="DeliveryMethod" use="optional"/>
                  <xs:attribute type="xs:byte" name="PrimeLineNo" use="optional"/>
                  <xs:attribute type="xs:string" name="ShipNode" use="optional"/>
                  <xs:attribute type="xs:float" name="OrderedQty" use="optional"/>
                  <xs:attribute type="xs:string" name="FulfillmentType" use="optional"/>
                  <xs:attribute type="xs:string" name="GiftFlag" use="optional"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="References">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Reference" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:string">
                      <xs:attribute type="xs:string" name="Value" use="optional"/>
                      <xs:attribute type="xs:string" name="Name" use="optional"/>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="PersonInfoBillTo">
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="xs:string">
                <xs:attribute type="xs:string" name="AddressLine1"/>
                <xs:attribute type="xs:string" name="FirstName"/>
                <xs:attribute type="xs:string" name="State"/>
                <xs:attribute type="xs:int" name="ZipCode"/>
                <xs:attribute type="xs:string" name="Country"/>
                <xs:attribute type="xs:string" name="City"/>
                <xs:attribute type="xs:string" name="LastName"/>
                <xs:attribute type="xs:string" name="EMailID"/>
                <xs:attribute type="xs:long" name="MobilePhone"/>
              </xs:extension>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
        <xs:element name="PersonInfoShipTo">
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="xs:string">
                <xs:attribute type="xs:string" name="AddressLine1"/>
                <xs:attribute type="xs:string" name="FirstName"/>
                <xs:attribute type="xs:string" name="State"/>
                <xs:attribute type="xs:int" name="ZipCode"/>
                <xs:attribute type="xs:string" name="Country"/>
                <xs:attribute type="xs:string" name="City"/>
                <xs:attribute type="xs:string" name="LastName"/>
                <xs:attribute type="xs:string" name="EMailID"/>
                <xs:attribute type="xs:long" name="MobilePhone"/>
              </xs:extension>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:attribute type="xs:string" name="EntryType"/>
      <xs:attribute type="xs:string" name="OrderType"/>
      <xs:attribute type="xs:string" name="AuthorizedClient"/>
      <xs:attribute type="xs:string" name="SellerOrganizationCode"/>
      <xs:attribute type="xs:byte" name="DocumentType"/>
      <xs:attribute type="xs:string" name="PaymentRuleId"/>
      <xs:attribute type="xs:byte" name="OrderNo"/>
      <xs:attribute type="xs:string" name="CustomerEMailID"/>
      <xs:attribute type="xs:string" name="ReqDeliveryDate"/>
      <xs:attribute type="xs:string" name="EnterpriseCode"/>
      <xs:attribute type="xs:dateTime" name="OrderDate"/>
    </xs:complexType>
  </xs:element>
</xs:schema>